!		unicode rxvt
URxvt*geometry:	115x40
!Inconsolata font
!URxvt*font:	xft:Inconsolata:pixelsize=14:antialiasing=true:hinting=true
!URxvt*boldFont:	xft:Inconsolata:pixelsize=14:antialiasing=true:hinting=true
!Tamsyn font
!URxvt*font:	xft:Misc Tamsyn:pixelsize=16:antialiasing=true:hinting=true
!URxvt*boldFont:	xft:Misc Tamsyn:pixelsize=16:antialiasing=true:hinting=true
!Dina font
!URxvt*font:	xft:Dina:pixelsize=10:antialiasing=false:hinting=true
!URxvt*boldFont:	xft:Dina:style=Bold:pixelsize=10:antialiasing=true:hinting=true
!Terminus font
URxvt*font:	xft:xos4 Terminus:pixelsize=16:antialiasing=true:hinting=true
URxvt*boldFont:	xft:xos4 Terminus:style=Bold:pixelsize=16:antialiasing=true:hinting=true
!Liberation font
!URxvt*font:	xft:Liberation Mono:pixelsize=14:antialiasing=false:hinting=true
!URxvt*boldFont:	xft:Liberation Mono:pixelsize=14:antialiasing=false:hinting=true
!Deja vu sans Mono font
!URxvt*font:	xft:Deja Vu Sans Mono:pixelsize=14:antialiasing=false:hinting=true
!URxvt*boldFont:	xft:Deja Vu Sans Mono:pixelsize=14:antialiasing=false:hinting=true
URxvt*depth:	32
URxvt*borderless:	1
URxvt*scrollBar:	false
URxvt*saveLines:	2000
URxvt.transparent:	false
URxvt*.shading:	22

!		Color Scheme
#URxvt*background: #2b2b2b
URxvt*background: #002b36
URxvt*foreground: #dedede

URxvt*colorUL: #86a2b0

!	Black
URxvt*color0:	#2e3436
URxvt*color8:	#555753
!	Red
URxvt*color1:	#cc0000
URxvt*color9:	#ief2929
!	Green
URxvt*color2:	#4e9a06
URxvt*color10:	#8ae234
!	Yellow
URxvt*color3:	#c4a000
URxvt*color11:	#fce94f
!	Blue
URxvt*color4:	#3465a4
URxvt*color12:	#729fcf
!	Magenta
URxvt*color5:	#75507b
URxvt*color13:	#ad7fa8
!	Cyan
URxvt*color6:	#06989a
URxvt*color14:	#34e2e2
!	White
URxvt*color7:	#d3d7cf
URxvt*color15:	#eeeeec
